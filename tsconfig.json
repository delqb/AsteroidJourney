{
    "compilerOptions": {
        "moduleResolution": "node",
        "moduleDetection": "force",
        "target": "ESNext",
        "module": "ESNext",
        "outDir": "js",
        "baseUrl": "./",
        "paths": {
            // Engine root
            "@fluidengine/*": [
                "engine/src/*"
            ],
            "@fluidlib/*": [
                "engine/src/lib/*"
            ],
            // 
            // AsteroidJourney
            // 
            "@asteroid/*": [
                "AsteroidJourney/src/*"
            ],
        }
    },
    "exclude": [
        "temp"
    ]
}