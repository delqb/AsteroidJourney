{
  "compilerOptions": {
    "moduleResolution": "node",
    "moduleDetection": "force",
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "js",
    "baseUrl": "./",
    "paths": {
      /* 
      Fluid
      */
      "@fluid/*": [
        "FluidEngine/src/*"
      ],
      /*  
      Fluid core
      */
      "@fluidcore/*": [
        "FluidEngine/src/core/*"
      ],
      /* 
      Fluid lib
      */
      "@fluidlib/*": [
        "FluidEngine/src/lib/*"
      ],
      /* 
      AsteroidJourney
      */
      "@asteroid/*": [
        "AsteroidJourney/src/*"
      ]
    }
  },
  "exclude": [
    "temp"
  ]
}